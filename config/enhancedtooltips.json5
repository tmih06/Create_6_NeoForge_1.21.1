		/* Shows the category of an item in a badge on its tooltip.
		   (default: true)
		*/
		"itemBadges": true,
		/* Adjusts the size of tooltips.
		   A scale of 100% displays the tooltip at its default size.
		   (default: 1.0)
		*/
		"scaleFactor": 0.800000011920929
	},
	"popUpAnimation": {
		/* Toggles the tooltip pop-up animation.
		   (default: true)
		*/
		"enabled": false,
		/* Duration of the pop-up animation in seconds.
		   (default: 1.5)
		*/
		"time": 1.5,
		/* Magnitude of the pop-up animation.
		   (default: 1.0)
		*/
		"magnitude": 1.0
	},
	"itemPreviewAnimation": {
		/* Toggles the item preview bouncing animation.
		   (default: true)
		*/
		"enabled": false,
		/* Duration of the item preview animation in seconds.
		   (default: 1.0)
		*/
